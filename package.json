{
    "name": "guangyizhou",
    "version": "1.0.37",
    "main": "main.js",
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "dev": "electron .",
        "electron-build": "vue-cli-service build && electron-packager ./dist 广艺舟 --arch=x64 --icon=dist/favicon.ico  --overwrite",
        "dist": "vue-cli-service build && electron-builder"
    },
    "repository": "https://github.com/xingyuelongchen/project --all",
    "keywords": [
        "Electron",
        "quick",
        "start",
        "tutorial",
        "demo"
    ],
    "dependencies": {
        "axios": "^0.19.2",
        "core-js": "^3.6.5",
        "cropperjs": "^1.5.7",
        "electron-updater": "^4.3.1",
        "element-ui": "^2.13.2",
        "file-saver": "^2.0.2",
        "is-electron": "^2.2.0",
        "less-loader": "^6.2.0",
        "request": "^2.88.2",
        "vue": "^2.6.11",
        "vue-cropper": "^0.5.5",
        "vue-router": "^3.2.0",
        "vuex": "^3.4.0",
        "vxe-table": "^2.9.15",
        "xe-utils": "^2.7.1",
        "xlsx": "^0.16.4"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.4.0",
        "@vue/cli-plugin-eslint": "~4.4.0",
        "@vue/cli-plugin-router": "~4.4.0",
        "@vue/cli-plugin-vuex": "~4.4.0",
        "@vue/cli-service": "~4.4.0",
        "@vue/eslint-config-prettier": "^6.0.0",
        "babel-eslint": "^10.1.0",
        "electron": "^9.1.2",
        "electron-builder": "^22.8.0",
        "electron-packager": "^15.0.0",
        "electron-webpack": "^2.8.2",
        "eslint": "^6.7.2",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-vue": "^6.2.2",
        "prettier": "^1.19.1",
        "sass": "^1.26.5",
        "sass-loader": "^8.0.2",
        "vue-template-compiler": "^2.6.11"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended",
            "@vue/prettier"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "rules": {}
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ],
    "build": {
        "asar": true,
        "productName": "广艺舟",
        "appId": "com.guangyizhou.pc",
        "copyright": "青龙",
        "directories": {
            "output": "build"
        },
        "nsis": {
            "oneClick": false,
            "perMachine": true,
            "allowElevation": true,
            "allowToChangeInstallationDirectory": true,
            "installerIcon": "./favicon.ico",
            "uninstallerIcon": "./favicon.ico",
            "uninstallDisplayName": "广艺舟",
            "installerHeaderIcon": "./favicon.ico",
            "createDesktopShortcut": "always",
            "createStartMenuShortcut": true,
            "warningsAsErrors": false,
            "shortcutName": "广艺舟"
        },
        "publish": [
            {
                "provider": "generic",
                "url": "",
                "channel": "latest"
            }
        ],
        "files": [
            "main.js",
            "dist/",
            "src/config.js",
            "preload.js",
            "favicon.ico",
            "store.json"
        ],
        "win": {
            "icon": "./favicon.ico",
            "target": [
                {
                    "target": "nsis",
                    "arch": [
                        "x64",
                        "ia32"
                    ]
                }
            ]
        }
    }
}